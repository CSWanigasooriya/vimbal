    "@angular/pwa": "^14.1.3",
    "@angular/router": "14.1.3",
    "@angular/service-worker": "14.1.3",
    "@metamask/onboarding": "^1.0.1",
    "@ngrx/component-store": "14.2.0",
    "@ngrx/effects": "14.2.0",
    "@ngrx/entity": "14.2.0",
    "@ngrx/router-store": "14.2.0",
    "@ngrx/store": "14.2.0",
    "@nrwl/angular": "^14.7.5",
    "buffer": "^6.0.3",
    "crypto-es": "^1.2.7",
